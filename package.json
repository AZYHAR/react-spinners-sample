{
    "name": "react-spinners-sample",
    "version": "1.0.0",
    "description": "",
    "main": "dist/bundle.cjs.js",
    "module": "dist/bundle.esm.js",
    "sideEffects": false,
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "rollup -c",
        "lint": "eslint $PWD/'**/*.js'",
        "lint:fix": "prettier-eslint $PWD/'**/*.js' --write",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
        "test:staged": "jest --findRelatedTests",
        "storybook": "start-storybook -p 9001 -c storybook",
        "storybook:build": "build-storybook -c storybook"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:fix",
            "npm run test:staged",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AZYHAR/react-spinners-sample.git"
    },
    "keywords": [
        "react",
        "css",
        "spinners"
    ],
    "author": "Anton Zyhar",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AZYHAR/react-spinners-sample/issues"
    },
    "homepage": "https://github.com/AZYHAR/react-spinners-sample#readme",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@storybook/addon-options": "^5.3.18",
        "@storybook/addons": "^5.3.18",
        "@storybook/react": "^5.3.18",
        "@storybook/theming": "^5.3.18",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.1.0",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.9.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "lint-staged": "^10.2.2",
        "prettier-eslint": "^9.0.1",
        "prettier-eslint-cli": "^5.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.13.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.7.6",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-terser": "^5.3.0"
    },
    "peerDependencies": {
        "react": ">=15"
    },
    "engines": {
        "node": ">=8"
    },
    "dependencies": {},
    "jest": {
        "moduleNameMapper": {
            "\\.(css|less)$": "<rootDir>/mocks/styles.js"
        }
    }
}
